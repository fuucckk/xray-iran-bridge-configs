{
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "domainMatcher": "hybrid",
    "balancers": [
      {
        "tag": "public-proxies",
        "selector": [
          "proxy-111"
        ],
        "strategy": {
          "type":"leastLoad",
          "settings": {
            "baselines": 10,
            "expected": 10
          }
        }
      }
    ],
    "rules": [
      {
        "inboundTag": [
          "proxy"
        ],
        "balancerTag": "public-proxies",
        "type": "field"
      }
    ]
  },
  "burstObservatory": {
    "subjectSelector":[
      "proxy-111"
    ],
    "pingConfig": {
      "destination": "http://www.google.com/gen_204",
      "interval": "15m",
      "connectivity": "http://www.google.com/gen_204",
      "timeout": "3s",
      "sampling": 2
    }
  }
}